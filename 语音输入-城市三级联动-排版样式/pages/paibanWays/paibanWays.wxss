.box {
  width: 100%;
  box-sizing: border-box;
  border: 4px solid red;
  margin-bottom: 50rpx;
}
.box>view{
  display: flex;
  justify-content: space-between;
}
.item {
  width: 100%;
  border: 4px solid green;
  margin-bottom: 50rpx;
}
.content{
  width: 100%;
  border: 4px solid blue;
  margin-bottom: 50rpx;
}
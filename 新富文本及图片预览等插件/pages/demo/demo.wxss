/* demo.wxss */
.loading-cell {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
}

.line-bounce {
  position: relative;
  display: inline-block;
  width: 100px;
  height: 100px;
}

.line-bounce text {
  position: absolute;
  top: calc(50% - 13px);
  display: block;
  width: 5px;
  height: 26px;
  background-color: #8fbbd8;
  animation: line-bounce 1s infinite linear;
}

.line-bounce text:nth-child(1) {
  left: 15px;
  animation-delay: 0.1s;
}

.line-bounce text:nth-child(2) {
  left: 30px;
  animation-delay: 0.25s;
}

.line-bounce text:nth-child(3) {
  left: 45px;
  animation-delay: 0.4s;
}

.line-bounce text:nth-child(4) {
  left: 60px;
  animation-delay: 0.55s;
}

.line-bounce text:nth-child(5) {
  left: 75px;
  animation-delay: 0.7s;
}

@keyframes line-bounce {
  0% {
    transform: scaleY(1);
  }

  50% {
    transform: scaleY(0.3);
  }

  100% {
    transform: scaleY(1);
  }
}

.search {
  position: fixed;
  top: 0;
  z-index: 999;
  display: flex;
  align-items: center;
  width: 100%;
  height: 50px;
  background-color: #f8f8f8;
  border-bottom: 1px solid #ccc;
}

.input {
  display: flex;
  width: 65%;
  height: 25px;
  padding: 5px;
  margin-left: 7.5px;
  line-height: 25px;
  background-color: white;
}

.button {
  display: flex;
  flex: 1;
  align-items: center;
  justify-content: center;
}


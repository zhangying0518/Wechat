.up_img_wrap{
  width: 90rpx;
  height: 90rpx;
  position: relative;
}
.up_img_wrap image{
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
}
.up_img_wrap text{
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  top: -22rpx;
  right: -22rpx;
  color: red;
  border: 1px solid red;
  border-radius: 50%;
}